{
    "name": "Demo Extension",
    "description": "Demo Extension Description",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "declarativeContent"
    ],
   
    "page_action": {
        "default_popup": "page_popup.html",
        "default_icon": {
            "16": "images/page_popup16.png",
            "32": "images/page_popup32.png",
            "48": "images/page_popup48.png",
            "128": "images/page_popup128.png"
        }
    },
    "icons": {
        "16": "images/page_popup16.png",
        "32": "images/page_popup32.png",
        "48": "images/page_popup48.png",
        "128": "images/page_popup128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens toolbar_popup.html"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}